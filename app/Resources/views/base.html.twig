<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <!--Import Google Icon Font-->
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <!--Import materialize.css-->
        <link rel="stylesheet" href="{{ asset('assets/css/materialize.min.css') }}" media="screen,projection">

    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body id="{% block id '' %}">
{% block body %}
    <footer>
        &copy; {{ 'now' | date('y') }} - Cupon
        <a href="{{ path('pagina_estatica', {'pagina': 'ayuda'}) }}">Ayuda</a> |
        <a href="{{ path('pagina_estatica', {'pagina': 'contacto'}) }}">Contacto</a> |
        <a href="{{ path('pagina_estatica', {'pagina': 'privacidad'}) }}">Privacidad</a> |
        <a href="{{ path('pagina_estatica', {'pagina': 'nosotros'}) }}">Sobre nosotros</a>
    </footer>
{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('assets/js/jquery.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/js/materialize.min.js') }}"></script>
{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
